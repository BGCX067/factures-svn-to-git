<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Button styleName="{style.important}" ui:field="goToNewInvoice" text="New Invoice"/>
		<g:Button styleName="{style.important}" ui:field="goToNewFixCost" text="New fix cost"/>
		<g:Button styleName="{style.important}" ui:field="goToInvoiceList" text="Invoices list"/>
		<g:Button styleName="{style.important}" ui:field="goToCostList" text="Fix costs list"/>
<!-- 		<g:Button styleName="{style.important}" ui:field="goToAll" text="list all"/> -->
	</g:HTMLPanel>
</ui:UiBinder> 